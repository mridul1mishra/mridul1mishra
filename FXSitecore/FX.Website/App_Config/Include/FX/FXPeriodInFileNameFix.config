<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <settings>
      <setting patch:instead="setting[@name='ItemNameValidation']" name="ItemNameValidation" value="^[\w\*\$\.][\w\s\-\$\.]*(\(\d{1,}\)){0,1}$" />
    </settings>
  </sitecore>
</configuration>